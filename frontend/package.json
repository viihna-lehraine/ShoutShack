{
	"name": "shoutshack_frontend",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"build": "npm run build:scss npm run build:tsc && vite build",
		"build:css": "sass src/styles/main.scss src/styles/main.css --no-source-map",
